<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProjectBeginnerOne" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/cornflowerblue</item>
        <item name="colorPrimaryVariant">@color/bisque</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/deepskyblue</item>
        <item name="colorSecondaryVariant">@color/darksalmon</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:layout_gravity">center</item>
        <item name="android:textAlignment">center</item>
        <item name="android:gravity">center</item>
    </style>
</resources>